<!--
  The base layout is the file that other layouts may inherit. This includes a
  pared-down, bare-bones skeleton, that includes the default page `head`, and
  blocks for content.
  -->

<!DOCTYPE html>
<html lang="en">

{% include "head.njk" %}

<body>

  <main class="main">
    {%- block main_content -%}
      <h1>{{- title -}}</h1>
      {{ content | safe }}
    {%- endblock main_content -%}
  </main>

</body>
</html>
